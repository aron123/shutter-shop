{
  "name": "shutter-shop",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.3.1",
    "express": "^4.16.4",
    "mongodb": "^3.2.3",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "mocha": "^6.1.3",
    "nodemon": "^1.18.11",
    "sinon": "^7.3.2"
  },
  "scripts": {
    "start": "NODE_ENV=production npm run build && npm run start-server",
    "test": "mocha server/**/*.test.js",
    "build-frontend": "react-scripts build",
    "test-frontend": "react-scripts test",
    "eject-frontend": "react-scripts eject",
    "start-server": "NODE_ENV=production node server",
    "start-frontend-dev": "react-scripts start",
    "start-server-dev": "nodemon server",
    "start-dev": "concurrently --kill-others-on-fail \"npm run start-server-dev\" \"npm run start-frontend-dev\""
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "proxy": "http://localhost:4000"
